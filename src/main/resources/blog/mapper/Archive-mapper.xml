<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.rainynight.site.blog.entity.Archive">

    <insert id="insert" parameterType="top.rainynight.site.blog.entity.Archive">
         insert into archive(name,body) values(#{name},#{body})
    </insert>

    <select id="select" resultType="top.rainynight.site.blog.entity.Archive" parameterType="top.rainynight.site.blog.entity.Archive">
        select * from archive where name=#{name}
    </select>
        </mapper>
